          })}\r\n            </div>\r\n          )\r\n        ) : (\r\n          <div className=\"job-grid\">\r\n            {allJobs?.data?.map((job) => {\r\n              return (\r\n                <div className=\"job-card\">\r\n                  <h2>{job.title}</h2>\r\n                  <p>{job.description}</p>\r\n                  <div className=\"bottom-card\">\r\n                    <p>\r\n                      <span>\r\n                        <img\r\n                          style={{\r\n                            width: \"12px\",\r\n                            height: \"12px\",\r\n                            marginRight: \"0.5rem\",\r\n                          }}\r\n                          src={locimg}\r\n                          alt=\"\"\r\n                        ></img>\r\n                      </span>\r\n                      {job.location}\r\n                    </p>\r\n                  </div>\r\n                </div>\r\n              );\r\n            })}\r\n          </div>\r\n        )}\r\n      </div>\r\n      {token !== null && localStorage.getItem(\"useRole\") === \"0\" ? null : (\r\n        <div className=\"page\">\r\n          <Pagination\r\n            activePage={activePage}\r\n            itemsCountPerPage={\r\n              token === null\r\n                ? allJobs?.metadata?.limit\r\n                : candidatejobs?.metadata?.limit\r\n            }\r\n            totalItemsCount={\r\n              token === null\r\n                ? allJobs?.metadata?.count\r\n                : candidatejobs?.metadata?.count\r\n            }\r\n            pageRangeDisplayed={5}\r\n            onChange={handlePageChange}\r\n            innerClass=\"pager\"\r\n          />\r\n        </div>\r\n      )}\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default Home;\r\n","C:\\Users\\verma\\OneDrive\\Documents\\Javascript & React.js\\sqaureboat\\src\\components\\recruit\\postjob.js",[],"C:\\Users\\verma\\OneDrive\\Documents\\Javascript & React.js\\sqaureboa